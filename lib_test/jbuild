(jbuild_version 1)

(executable
  ((name test_bitarray)
   (public_name test_bitarray)
   (libraries (bitarray alcotest))
   (modules (test_bitarray))))

(executable
  ((name bench_obitarray)
   (public_name bench_obitarray)
   (libraries (bitarray core core_bench))
   (modules (bench_obitarray))))

(alias
 ((name runtest)
  (deps (test_bitarray.exe))
  (action (run ${<}))))
